//the menu skin json file - uses menuSkin.png & menuSkin.atlas files
{
  com.badlogic.gdx.graphics.g2d.BitmapFont: {
    default-font: { file: fonts/stencil-med.fnt }
  },
  com.badlogic.gdx.graphics.Color: {
    green: { a: 1, b: 0, g: 1, r: 0 },
    white: { a: 1, b: 1, g: 1, r: 1 },
    red: { a: 1, b: 0, g: 0, r: 1 },
    black: { a: 1, b: 0, g: 0, r: 0 }
  },
  com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
    calc: { up: calc, font: default-font, fontColor: black},
    supply: {up: supplies, font: default-font, fontColor: black}
  },
  com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
    default: { font: default-font, fontColor: white, background: title }
  }
}